{
  "name": "flow-state-gui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js --fix",
    "format": "prettier --write src/",
    "server": "node server.js",
    "start": "npm run build && npm run server",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "dev:real": "cp .env.real .env && npm run dev:full",
    "dev:mock": "cp .env .env.local && npm run dev"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vite": "^5.0.12",
    "vite-plugin-vuetify": "^2.0.1"
  }
}